{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        { "source": "/catalogue", "destination": "/connexion.html" },
        { "source": "/death", "destination": "/views/index.ejs" },
        { "source": "/proxy/:match*", "destination": "https://example.com/:match*" },
        {
          "source": "/:path((?!uk/).*)",
          "has": [
            {
              "type": "header",
              "key": "x-vercel-ip-country",
              "value": "GB"
            }
          ],
          "destination": "/uk/:path*"
        }
      ]
  }